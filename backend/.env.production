# Configuration Production
NODE_ENV=production
PORT=4000

# Base de données PostgreSQL (à modifier avec vos vraies valeurs)
DATABASE_URL="postgresql://USER:PASSWORD@HOST:5432/DATABASE_NAME?schema=public"

# JWT Secret (générez un secret fort et unique)
JWT_SECRET="CHANGEZ_MOI_PAR_UN_SECRET_TRES_LONG_ET_SECURISE"

# CORS - URL de votre frontend
FRONTEND_URL="https://votre-domaine.com"

# Optionnel : Uploads
MAX_FILE_SIZE=5242880
UPLOAD_DIR=./uploads
